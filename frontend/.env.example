# Example values for local development.
VITE_API_BASE=http://127.0.0.1:5001/demo-crowdpm/us-central1/crowdpmApi
VITE_GOOGLE_MAPS_API_KEY=replace-with-google-maps-key
VITE_GOOGLE_MAP_ID=replace-with-google-vector-map-id

# Firebase web app config
VITE_FIREBASE_API_KEY=fake-demo-api-key
VITE_FIREBASE_AUTH_DOMAIN=demo-crowdpm.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=demo-crowdpm
VITE_FIREBASE_STORAGE_BUCKET=demo-crowdpm.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=1234567890
VITE_FIREBASE_APP_ID=1:1234567890:web:demo1234567890

# Optional: point Firebase Auth SDK at the local emulator when running `pnpm dev`.
# Comment out to hit a hosted Firebase project instead.
VITE_FIREBASE_AUTH_EMULATOR_HOST=127.0.0.1:9099
